<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.edu.utar.mad_individual">

    <application
        android:allowBackup="true"
        android:icon="@drawable/bill"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.MAD_Individual">

        <activity android:name=".activity_history"/>
        <activity android:name=".activity_result3" />
        <activity android:name=".activity_result2" />
        <activity android:name=".activity_result" />
        <activity android:name=".activity_way" />
        <activity
            android:name=".activity_count3"
            android:parentActivityName=".activity_result3"/>
        <activity
            android:name=".activity_count2"
            android:parentActivityName=".activity_result2" />
        <activity
            android:name=".activity_count"
            android:parentActivityName=".activity_result" />
        <activity
            android:name=".activity_splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>